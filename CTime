public class CTime implements Time,Comparable<CTime> {
	
	private int hours;
	private int mins;
	private int secs;
	private int mSecs;
	

	@Override
	public int getHH() {
		return hours;
	}

	@Override
	public int getMM() {
		return mins;
	}

	@Override
	public int getSS() {
		return secs;
	}

	@Override
	public int getMS() {
		return mSecs;
	}

	@Override
	public void setHH(int hh) {
		this.hours = hh;
	}

	@Override
	public void setMM(int mm) {
		this.mins = mm;
	}

	@Override
	public void setSS(int ss) {
		this.secs = ss;
	}

	@Override
	public void setMS(int ms) {
		this.mSecs = ms;
	}

	@Override
	public int compareTo(CTime o) {
		// TODO Auto-generated method stub
		if(this.hours>o.getHH())
			return 1;
		if(this.hours<o.getHH())
			return -1;
		
		if(this.mins>o.getMM())
			return 1;
		if(this.mins<o.getMM())
			return -1;
		
		if(this.secs>o.getSS())
			return 1;
		if(this.secs<o.getSS())
			return -1;
		
		if(this.mSecs>o.getMS())
			return 1;
		if(this.mSecs<o.getMS())
			return -1;
		
		return 0;
	}	

}







